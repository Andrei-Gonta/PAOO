#include <iostream>
#include <string>
using namespace std;

class Exercitiu
{
    private: 
    
        string nume;
        int sets;
        int reps;
        int kg;
        
    public:
    
        Exercitiu(string nume, int sets, int reps, int kg) :
        nume(nume), sets(sets), reps(reps), kg(kg)
        {
            cout<<"Exercitiul "<<nume<<" a fost creat cu succes!"<<endl;
        }
        
    
};




int main()
{
    Exercitiu bench_press("Bench press", 4, 8, 80);
    return 0;
}
